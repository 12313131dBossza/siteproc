{
  "name": "siteproc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run --coverage tests",
    "analyze:accessibility": "npx lighthouse http://localhost:3000 --only-categories=accessibility --quiet --chrome-flags='--headless=new' || true",
    "loadtest:publicQuote": "node scripts/loadtest-public-quote.js",
  "sql:change-orders": "node scripts/run-sql.js sql/change-orders.sql",
  "sql:projects": "node scripts/run-sql.js sql/projects.sql",
    "scan:secrets": "npx @trufflesecurity/trufflehog filesystem --only-verified --json . || true",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --project=chromium --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:edge": "set PW_CHANNEL=msedge&& playwright test",
    "e2e:chrome": "set PW_CHANNEL=chrome&& playwright test",
    "playwright:install": "playwright install chromium"
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.3.0",
    "@sendgrid/mail": "^8.1.5",
    "@sentry/nextjs": "^8.26.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.542.0",
    "next": "15.5.0",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.5.3",
    "pdf-lib": "^1.17.1",
    "posthog-js": "^1.260.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "sonner": "^2.0.7",
    "zod": "^4.0.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "playwright": "^1.55.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0"
  }
}
